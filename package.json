{
  "name": "bryce.io",
  "version": "1.0.5",
  "scripts": {
    "start": "env NODE_ENV=development parcel src/index.html",
    "build": "rm -rf dist && env NODE_ENV=production parcel build src/index.html --detailed-report --no-source-maps --no-content-hash --public-url https://bryce.io",
    "lint": "eslint . --ext .js --fix",
    "snap": "react-snap"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--single-process"
    ]
  },
  "author": {
    "name": "Bryce Dorn",
    "url": "https://www.bryce.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brycedorn/brycedorn.github.io"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "homepage": "https://bryce.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "parcel": "^2.5.0",
    "react-snap": "^1.23.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "preact": "^10.7.2",
    "process": "^0.11.10",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-meta-tags": "^1.0.1",
    "use-draggable-hook": "^0.2.0"
  }
}
