{
  "name": "bryce.io",
  "version": "1.0.7",
  "scripts": {
    "start": "rm -rf dist .parcel-cache && env NODE_ENV=development parcel src/index.html",
    "build": "rm -rf dist && env NODE_ENV=production parcel build src/index.html --no-source-maps --no-content-hash --public-url https://bryce.io && react-snap",
    "build-dev": "rm -rf dist .parcel-cache && parcel build src/index.html --no-content-hash && react-snap && npm run remove-tags",
    "lint": "eslint . --ext .js --fix",
    "remove-tags": "node src/js/remove-tags.js"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    }
  },
  "author": {
    "name": "Bryce Dorn",
    "url": "https://www.bryce.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brycedorn/brycedorn.github.io"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "homepage": "https://bryce.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "@parcel/transformer-css": "^2.11.0",
    "eslint": "^8.17.0",
    "eslint-plugin-css": "^0.9.1",
    "eslint-plugin-react": "^7.30.0",
    "node-html-parser": "^6.1.12",
    "parcel": "^2.5.0",
    "react-snap": "^1.23.0"
  },
  "dependencies": {
    "preact": "^10.19.3",
    "process": "^0.11.10",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-meta-tags": "^1.0.1"
  }
}
