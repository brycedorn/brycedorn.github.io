{
  "name": "bryce.io",
  "version": "1.0.3",
  "scripts": {
    "start": "env NODE_ENV=development parcel src/index.html",
    "build": "rm -rf dist && env NODE_ENV=production parcel build src/index.html --detailed-report --no-source-maps",
    "lint": "eslint . --ext .js --fix",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    }
  },
  "author": {
    "name": "Bryce Dorn",
    "url": "https://www.bryce.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brycedorn/brycedorn.github.io"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "homepage": "https://bryce.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "eslint": "^8.15.0",
    "eslint-plugin-react": "^7.29.4",
    "parcel": "^2.5.0",
    "preact": "^10.1.1",
    "preact-compat": "^3.19.0",
    "react-snap": "^1.23.0",
    "sass": "^1.30.0"
  },
  "dependencies": {
    "framer-motion": "^6.3.3",
    "process": "^0.11.10",
    "react-helmet": "^6.1.0"
  }
}
